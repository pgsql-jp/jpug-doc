   <tgroup cols="4">
    <thead>
     <row>
<!--
      <entry>Name</entry>
      <entry>Type</entry>
      <entry>References</entry>
      <entry>Description</entry>
-->
      <entry>名前</entry>
      <entry>型</entry>
      <entry>参照先</entry>

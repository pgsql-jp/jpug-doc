<!--
      <entry>Time zone name</entry>
-->
      <entry>時間帯名</entry>
     </row>
     <row>
      <entry><structfield>abbrev</structfield></entry>
      <entry><type>text</type></entry>
<!--
      <entry>Time zone abbreviation</entry>

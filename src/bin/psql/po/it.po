msgid "  \\ds[S+] [PATTERN]      list sequences\n"
msgstr "  \\ds[S+] [MODELLO]      elenca le sequenze\n"

#: help.c:244
#, c-format
msgid "  \\dt[S+] [PATTERN]      list tables\n"
msgstr "  \\dt[S+] [MODELLO]      elenca le tabelle\n"

